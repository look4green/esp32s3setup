void setup() {
  Serial.begin(115200);
  pinMode(TFT_BL, OUTPUT);
  digitalWrite(TFT_BL, HIGH); 

  tft.initR(INITR_GREENTAB); 
  tft.setRotation(0); 
  tft.fillScreen(ST77XX_BLACK);

  // --- LOGO SCENE ---
  // We use 0,0 to start at the very top left corner
  // We use 128, 160 to try and draw the FULL screen if needed
  tft.drawBitmap(0, 0, kejuLogo, 128, 128, KEJU_CYAN); 
  
  delay(3000); 

// --- PHASE 2: BRANDING & SYSTEM LOG ---
  tft.fillScreen(ST77XX_BLACK); 
  
  // 1. Version Tag (Bottom Right)
  tft.setTextSize(1);
  tft.setTextColor(DARK_GREY);
  tft.setCursor(85, 150);
  tft.print("v1.0.4");

  // 2. Main Title
  tft.setCursor(40, 40);
  tft.setTextColor(ST77XX_WHITE);
  tft.setTextSize(2);
  tft.print("KEJU");
  
  // 3. Subtitle
  tft.setCursor(28, 65);
  tft.setTextSize(1);
  tft.setTextColor(DARK_GREY);
  tft.print("TECHNOLOGIES");

  // 4. Loading Bar & Scrolling Logs
  tft.drawRect(14, 110, 102, 10, DARK_GREY);
  
  const char* logs[] = {"Init Core...", "PSRAM Check...", "SPI Bus OK", "System Ready"};
  
  for(int i=0; i<=100; i++) {
    tft.fillRect(15, 111, i, 8, ST77XX_GREEN);
    
    // Show different log messages based on progress
    tft.setTextSize(1);
    tft.setTextColor(KEJU_CYAN, ST77XX_BLACK);
    tft.setCursor(35, 95); // Space above the bar for logs
    
    if (i < 25)       tft.print(logs[0]);
    else if (i < 50)  tft.print(logs[1]);
    else if (i < 75)  tft.print(logs[2]);
    else              tft.print(logs[3]);

    // Percentage
    tft.setCursor(52, 125);
    tft.printf("%d%% ", i);
    
    delay(25); // Slightly slower to let users read the logs
  }

  delay(1000);
  tft.fillScreen(ST77XX_BLACK); // Final clear before Monitor starts
}
// --- EVERYTHING ABOVE THIS (setup, logo data, etc.) REMAINS THE SAME ---

void loop() {
  // 1. Get Temperature
  float temp_c = temperatureRead();
  
  // 2. Refresh UI Header
  tft.fillRect(0, 0, 128, 20, ST77XX_BLUE);
  tft.setTextColor(ST77XX_WHITE);
  tft.setTextSize(1);
  tft.setCursor(38, 6);
  tft.print("KEJU CORE");

  // 3. Update Temp Stats
  tft.setCursor(28, 35);
  tft.setTextSize(2);
  tft.setTextColor(ST77XX_RED, ST77XX_BLACK);
  tft.printf("%.1f C", temp_c);

  // 4. Update RAM Stats
  tft.setCursor(30, 65);
  tft.setTextSize(1);
  tft.setTextColor(ST77XX_CYAN, ST77XX_BLACK);
  tft.printf("MEM: %u KB", ESP.getFreePsram() / 1024);

  // 5. Core Heartbeats (The Techy Blinking Lights)
  tft.setTextSize(1);
  tft.setTextColor(DARK_GREY);
  tft.setCursor(10, 140);
  tft.print("C0:");
  tft.setCursor(70, 140);
  tft.print("C1:");

  uint16_t c0 = (millis() % 1000 < 500) ? ST77XX_GREEN : DARK_GREY;
  uint16_t c1 = (millis() % 800 < 400) ? ST77XX_GREEN : DARK_GREY;
  
  tft.fillRect(30, 140, 8, 8, c0);
  tft.fillRect(90, 140, 8, 8, c1);

  delay(200); // Slow down the loop slightly to prevent flickering
}
